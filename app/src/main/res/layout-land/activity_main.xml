<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/fondohorizontalinicio"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity"
    android:id="@+id/inicio_land">

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:text="Registro"
        android:id="@+id/registro"
        android:longClickable="true"
        android:clickable="true"
        android:src="@drawable/botonregistro160x60"
        android:background="@null"
        android:layout_centerVertical="true"
        android:layout_toRightOf="@+id/imageView2"
        android:layout_toEndOf="@+id/imageView2" />

    <ImageButton
        android:layout_width="160dp"
        android:layout_height="60dp"
        android:text="Login"
        android:src="@drawable/botondelogin160px"
        android:id="@+id/login"
        android:clickable="true"
        android:background="@null"
        android:layout_below="@+id/registro"
        android:layout_alignLeft="@+id/registro"
        android:layout_alignStart="@+id/registro" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:gravity="center"
        android:padding="20dp"
        android:src="@drawable/textoinicial1"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />


</RelativeLayout>
